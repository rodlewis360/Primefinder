
def fib(n):
    list = []
    a, b = 1, 2
    while a < n:
        a, b = b, a+b
        list.append(b)
    return list

def prime(a, list):
    str(list)
    b = 1
    d = 0
    p = 0
    while d <= a:
        c = 0
        while c <= 4:
            p += list[b]
            b += 1
            c += 1
        d += 1
    return p
 
 
        
